.env.backup
.env.local
.env.*.local
.env.docker
.env.production
.env.development
.env.test
.env.backup

# Docker files
docker-compose*.yml
Dockerfile*
.dockerignore

# Git
.git
.gitignore
.gitattributes
.github

# Prisma
prisma/*.db
prisma/*.db-journal
prisma/migrations/*/*.sql

# Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Build outputs
dist
build
coverage
.next
out

# IDE
.vscode
.idea
*.swp
*.swo
*~
.DS_Store

# Testing
coverage
.nyc_output
*.test.ts
*.spec.ts
test
tests

# Documentation
README.md
*.md
docs

# Uploads (will be mounted as volume)
uploads/*

